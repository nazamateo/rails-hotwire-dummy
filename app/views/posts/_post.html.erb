<%= turbo_frame_tag dom_id(post) do %>
  <div class="posts" id="<%= dom_id post %>">
    <p>
      <strong>Name:</strong>
      <%= post.name %>
    </p>
    <p>
      <strong>Title:</strong>
      <%= post.title %>
    </p>
    <p>
      <strong>Content:</strong>
      <%= post.content %>
    </p>
    <%= link_to "edit", edit_post_path(post)%>
    <%= button_to "Destroy this post", post, method: :delete %>
  </div>
<%end%>
